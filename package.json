{
  "name": "signflow",
  "version": "0.0.1",
  "description": "signflow prototype",
  "main": "index.js",
  "scripts": {
    "apiServer": "node src/js/apiServer.js",
    "test": "karma start",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    "validate": "npm-run-all --parallel validate-webpack:* test",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "clean-dist": "rimraf dist",
    "copy-files": "cpy src/site/* dist",
    "copy-files2": "cpy src/api/* dist/api",
    "clean-and-copy": "npm run clean-dist && npm run copy-files && npm run copy-files2",
    "prestart": "npm run clean-and-copy",
    "start": "webpack-dev-server --env.dev --content-base dist",
    "build": "webpack --env.dev",
    "build:prod": "webpack --env.prod -p"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "blueimp-load-image": "git+https://github.com/blueimp/JavaScript-Load-Image.git",
    "chart.js": "^2.4.0",
    "clipboard-js": "^0.3.2",
    "es6-promise": "^4.0.5",
    "localforage": "^1.4.3",
    "material-design-icons-iconfont": "^3.0.0",
    "react": "^15.3.1",
    "react-chartjs-2": "^1.5.1",
    "react-dom": "^15.3.1",
    "react-hammerjs": "^0.5.0",
    "react-sticky-position": "^2.0.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.26.0",
    "eslint": "^3.4.0",
    "faker": "^3.1.0",
    "file-loader": "^0.9.0",
    "json-server": "^0.9.4",
    "karma": "^1.2.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "node-sass": "^3.9.0",
    "npm-run-all": "^3.1.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-server": "^2.1.0-beta.11",
    "webpack-validator": "^2.2.9"
  }
}
